proc            /proc           proc    defaults               0   0
/dev/mmcblk0p1  /boot           vfat    defaults,ro,noatime,nodiratime,errors=remount-ro  0   2
/dev/mmcblk0p2  /               ext4    defaults,ro,noatime,nodiratime,errors=remount-ro  0   1
tmpfs           /tmp            tmpfs   nodev,nosuid,size=50M  0   0
